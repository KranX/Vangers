																		\
				selectable_obj													\
				Element STRING {												\
					id		$GAME_STRING_ID 									\
					align_x 	center											\
																		\
					terrain_num	$TERRAIN2										\
																		\
					font		$GAME_FNT										\
					null_level	$DEFAULT_NULL										\
					string		$GAME_OBJ_ID										\
				}														\
				Event { 													\
					if_selected												\
					key $VK_LBUTTON 											\
					key $VK_RETURN												\
					key $VK_SPACE												\
					time { $PRESS_OBJ_TIME + $MOVE2SCREEN_TIME + 1 }							\
					EvComm {												\
						low_obj 	$GAME_OBJ_NAME									\
																		\
						start_time	0										\
						time		$PRESS_OBJ_TIME 								\
					}													\
					EvComm {												\
						ext_event	$iEXT_SELECT_GAME $GAME_Y							\
						start_time	0										\
					}													\
					EvComm {												\
						move2screen	$GAME_SCR									\
						start_time	$PRESS_OBJ_TIME 								\
						time		$MOVE2SCREEN_TIME								\
					}													\
				}														\
																		\
				Event { 													\
					if_not_selected 											\
					key $VK_RBUTTON 											\
																		\
					time { $PRESS_OBJ_TIME + $SMOOTH_OBJ_TIME + $HIDE_OBJ_TIME + $MOVE2SCREEN_TIME + 1 }			\
					EvComm {												\
						jump_obj	$GAME_OBJ_NAME									\
						start_time	$PRESS_OBJ_TIME 								\
					}													\
					EvComm {												\
						ext_event	$iEXT_SELECT_GAME $GAME_Y							\
						start_time	0										\
					}													\
					EvComm {												\
						smooth_obj	$GAME_OBJ_NAME	4								\
