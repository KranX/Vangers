	Object "Load Game Option"
	{
		x	$LOAD_GAME_X
		y	$LOAD_GAME_Y

		selectable_obj
		Element STRING {
			align_x center

			terrain_num	$TERRAIN2

			font		$MAIN_FNT
			null_level	$DEFAULT_NULL
			string		$iSTR_LOAD_GAME
		}

		Event {
			if_selected
			key $VK_SPACE
			key $VK_RETURN
			key $VK_LBUTTON

			time { $PRESS_OBJ_TIME + MOVE2SCREEN_TIME + 1 }
			EvComm {
				low_obj 	"Load Game Option"
				time		$PRESS_OBJ_TIME
				start_time	0
			}
			EvComm {
				ext_event	$iEXT_PREPARE_SLOTS	0
				start_time	1
			}
			EvComm {
				move2screen	"Loading screen"
				start_time	$PRESS_OBJ_TIME
				time		$MOVE2SCREEN_TIME
			}
		}
		Event {
			if_not_selected
			key $VK_RBUTTON

			time { $SMOOTH_OBJ_TIME + $PRESS_OBJ_TIME + $HIDE_OBJ_TIME + MOVE2SCREEN_TIME + 1 }
			EvComm {
				jump_obj	"Load Game Option"
				start_time	$SMOOTH_OBJ_TIME
			}
			EvComm {
				smooth_obj	"Load Game Option" 4
				start_time	$PRESS_OBJ_TIME
				time		$SMOOTH_OBJ_TIME
			}
			EvComm {
				hide_obj       "Load Game Option"
				start_time	{ $SMOOTH_OBJ_TIME + $PRESS_OBJ_TIME }
				time		$HIDE_OBJ_TIME
			}
			EvComm {
				low_obj 	"SELECTED_OBJ"
				start_time	0
				time		$PRESS_OBJ_TIME
			}
			EvComm {
				rise_obj	"Load Game Option"
				start_time	0
				time		$PRESS_OBJ_TIME
			}
			EvComm {
				ext_event	$iEXT_PREPARE_SLOTS	0
				start_time	1
			}
			EvComm {
				move2screen	"Loading screen"
				start_time	{ $SMOOTH_OBJ_TIME + $PRESS_OBJ_TIME + $HIDE_OBJ_TIME }
				time		$MOVE2SCREEN_TIME
			}
		}
		Event {
			if_selected
			key $VK_RBUTTON

			time { $SMOOTH_OBJ_TIME + $HIDE_OBJ_TIME + MOVE2SCREEN_TIME + 1 }
			EvComm {
				smooth_obj	"Load Game Option" 4
				start_time	0
				time		$SMOOTH_OBJ_TIME
			}
			EvComm {
				hide_obj	"Load Game Option"
				start_time	$SMOOTH_OBJ_TIME
				time		$HIDE_OBJ_TIME
			}
			EvComm {
				ext_event	$iEXT_PREPARE_SLOTS	0
				start_time	1
			}
			EvComm {
				move2screen	"Loading screen"
				start_time	{ $SMOOTH_OBJ_TIME + $HIDE_OBJ_TIME }
				time		$MOVE2SCREEN_TIME
			}
		}

		#define OBJ1	"SELECTED_OBJ"
		#define OBJ2	"Load Game Option"
		Event {
			if_not_selected
			key $VK_LBUTTON
			key $VK_MOVE

			$EV_CHANGE_OBJ
		}

		#define OBJ1	"Load Game Option"
		#define OBJ2	"Options Option"
		Event {
			if_selected
			key $VK_DOWN
			key $VK_TAB

			$EV_CHANGE_OBJ
		}

		#define OBJ1	"Load Game Option"
		#define OBJ2	"New Game Option"
		Event {
			if_selected
			key $VK_UP

			$EV_CHANGE_OBJ
		} 
	}

